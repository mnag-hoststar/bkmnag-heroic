{% extends "layout.twig" %}
{% block content %}

<nav class="bk-nav">
	<div class="clearfix">
		{{widget('extendednavigation', 'navigationheader', {"align":"center"})|raw}}
	</div>
</nav>

<header class="bk-header container">
	<div class="clearfix">
		{{widget('logo', 'logo', {"defaultlogo": asset('images/logo.png')})|raw}}
		{{widget('companyname', 'companyname', {"business": "profile"})|raw}}
	</div>
</header>

<section class="bk-section container">
	{{widget('feature', 'featurehome', {"header": "profile", "strapline":"profile", "buttonText":"Go to Gallery","defaultBgImg": asset('images/feature-bg.jpg'), 'positionTopPercentage': '50','showBtn': true})|raw}}
</section>

<div class="main container clearfix">
	{{zone('main')|raw}}
</div>

<footer class="bk-footer">
	<div class="container clearfix">
		<div class="footerleft">
		{{widget('profile', 'footerprofileleft', { 'profileType' : 'socialicons' })|raw}}
		</div>
		<div class="footerright">
		{{widget('profile', 'footerprofileright', { 'profileType':'copyright', 'copyright':'profile'})|raw}}
		</div>
	</div>
</footer>

{% endblock %}